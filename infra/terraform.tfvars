# fill out the fild below before running the terraform script

# download the service account key, put the path to the service account key below
gcp_svc_key = "/home/path/to/service/account/key.json"
# put the project ID below
gcp_project = "project_ID"
# provide the region below. it does not have to be us-central1, but in GCP resources in us-central1 are cheaper or even free!
gcp_region = "us-central"
